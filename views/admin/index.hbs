<div class="container-fluid position-relative d-flex p-0">

	<!-- Sidebar Start -->
	{{> adminSidebar}}
	<!-- Sidebar End -->
	

	<!-- Content Start -->
	<div class="content">
		<!-- Navbar Start -->
		{{> adminNavbar}}
		<!-- Navbar End -->
		
		<!-- Sale & Revenue Start -->
		<div class="container-fluid pt-4 px-4">
			<div class="row g-4">
				<div class="col-sm-6 col-xl-3">
					<div
						class="bg-secondary rounded d-flex align-items-center justify-content-between p-4"
					>
						<i class="fa fa-users fa-3x text-primary"></i>
						<div class="ms-3">
							<p class="mb-2">All Users</p>
							<h6 class="mb-0">Jami: {{allUsersSize}} ta</h6>
						</div>
					</div>
				</div>
				<div class="col-sm-6 col-xl-3">
					<div
						class="bg-secondary rounded d-flex align-items-center justify-content-between p-4"
					>
						<i class="fa fa-home fa-3x text-primary"></i>
						<div class="ms-3">
							<p class="mb-2">All Products</p>
							<h6 class="mb-0">Jami : {{allProductsSize}} ta</h6>
						</div>
					</div>
				</div>
				<div class="col-sm-6 col-xl-3">
					<div
						class="bg-secondary rounded d-flex align-items-center justify-content-between p-4"
					>
						<i class="fa fa-user-shield fa-3x text-primary"></i>
						<div class="ms-3">
							<p class="mb-2">All Admins</p>
							<h6 class="mb-0">Jami : {{adminsSize}} ta</h6>
						</div>
					</div>
				</div>
				<div class="col-sm-6 col-xl-3">
					<div
					class="bg-secondary rounded d-flex align-items-center justify-content-between p-4">
						<i class="fa fa-user-secret fa-3x text-primary"></i>
						<div class="ms-3">
							<p class="mb-2">All Owners</p>
							<h6 class="mb-0">Jami : {{ownersSize}} ta</h6>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- Sale & Revenue End -->

		<!-- Widgets Start -->
		<div class="container-fluid pt-4 px-4">
			<div class="row g-4">
				 <div class="col-sm-12 col-xl-6">
						<div class="bg-secondary rounded h-100 p-4">
							<h6 class="mb-4">Last Users</h6>
							<div class="owl-carousel testimonial-carousel">
								{{#each allUsers}}
									<div class="testimonial-item text-center">
											<img class="img-fluid rounded-circle mx-auto mb-4" src="{{image}}" style="width: 100px; height: 100px;">
											<h5 class="mb-1">{{firstName}} {{lastName}}</h5>
											<p>Joined at : {{ formatDate  createdAt}}</p>
											<p class="mb-0">This user is join our website {{formatDate createdAt 1}} ago</p>
									</div>
								{{/each}}
							</div>
					</div>
			</div>
				<div class="col-sm-12 col-md-6 col-xl-6">
					<div class="h-100 bg-secondary rounded p-4">
						<div
							class="d-flex align-items-center justify-content-between mb-4"
						>
							<h6 class="mb-0">Calender</h6>
							<a href="">Show All</a>
						</div>
						<div id="calender"></div>
					</div>
				</div>
			</div>
		</div>
		<!-- Widgets End -->
		
		<!-- Footer Start -->
		{{> adminFoot}}
		<!-- Footer End -->
	</div>
	<!-- Content End -->

	<!-- Back to Top -->
	<a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i
			class="bi bi-arrow-up"
		></i></a>
</div>